<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<!-- T_GNOMTECWIDGETCONTAINERTOOLBOX: Standard template for GnomTECWidgetContainerToolbox-->
	<Frame name="T_GNOMTECWIDGETCONTAINERTOOLBOX" movable="true" resizable="true" clampedToScreen="true" toplevel="true" enableMouse="true" hidden="true" virtual="true" inherits="BackdropTemplate">
		<Size x="64" y="64"/>
		<ResizeBounds>
			<minResize>
				<AbsDimension x="16" y="16"/>
			</minResize>
			<maxResize>
				<RelDimension x="1.0" y="1.0"/>
			</maxResize>
		</ResizeBounds>
		<KeyValues>
			<KeyValue key="backdropInfo" value="GNOMTEC_BACKDROP_WIDGET_CONTAINER_TOOLBOX" type="global"/>
			<KeyValue key="backdropColor" value="GNOMTEC_BACKDROP_WIDGET_CONTAINER_TOOLBOX_COLOR" type="global"/>
		</KeyValues>
		<Frames>
    		<Frame name="$parentattachFrame" parentKey="attachFrame">
				<Anchors>
   		     <Anchor point="TOPLEFT">
     			     <Offset>
 			           <AbsDimension x="4" y="-4"/>
 			        </Offset>
 			     </Anchor>
   		     <Anchor point="BOTTOMRIGHT">
     			     <Offset>
 			           <AbsDimension x="-4" y="4"/>
 			        </Offset>
 			     </Anchor>
				</Anchors>
 				<Frames>
					<Frame name="$parentLeft" parentKey="left">
						<Size x="1" y="1"/>
						<Anchors>
						  <Anchor point="TOPLEFT">
							  <Offset>
								  <AbsDimension x="0" y="-25"/>
							  </Offset>
						  </Anchor>
						  <Anchor point="BOTTOMLEFT">
							  <Offset>
								  <AbsDimension x="0" y="25"/>
							  </Offset>
						  </Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentRight" parentKey="right">
						<Size x="1" y="1"/>
						<Anchors>
						  <Anchor point="TOPRIGHT">
							  <Offset>
								  <AbsDimension x="0" y="-25"/>
							  </Offset>
						  </Anchor>
						  <Anchor point="BOTTOMRIGHT">
							  <Offset>
								  <AbsDimension x="0" y="25"/>
							  </Offset>
						  </Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentTop" parentKey="top">
						<Size x="1" y="1"/>
						<Anchors>
						  <Anchor point="TOPLEFT">
							  <Offset>
								  <AbsDimension x="25" y="0"/>
							  </Offset>
						  </Anchor>
						  <Anchor point="TOPRIGHT">
							  <Offset>
								  <AbsDimension x="-25" y="0"/>
							  </Offset>
						  </Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentBottom" parentKey="bottom">
						<Size x="1" y="1"/>
						<Anchors>
						  <Anchor point="BOTTOMLEFT">
							  <Offset>
								  <AbsDimension x="25" y="0"/>
							  </Offset>
						  </Anchor>
						  <Anchor point="BOTTOMRIGHT">
							  <Offset>
								  <AbsDimension x="-25" y="0"/>
							  </Offset>
						  </Anchor>
						</Anchors>
					</Frame>
				</Frames>
			</Frame>
   		<Frame name="$parentcontainerFrame" parentKey="containerFrame">
				<Anchors>
   		     <Anchor point="TOPLEFT">
     			     <Offset>
 			           <AbsDimension x="4" y="-4"/>
 			        </Offset>
 			     </Anchor>
   		     <Anchor point="BOTTOMRIGHT">
     			     <Offset>
 			           <AbsDimension x="-4" y="4"/>
 			        </Offset>
 			     </Anchor>
 			  </Anchors>
			</Frame>
<!-- we have some issues here and as we didn't use it yet it's removed for now
   		<Frame name="$parentcontainerHelpFrame" parentKey="helpFrame" inherits="T_GNOMTECWIDGET_HELPFRAME">
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentcontainerFrame" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT" relativeTo="$parentcontainerFrame" x="0" y="0"/>
				</Anchors>
			</Frame>
-->
		</Frames>
	</Frame>
</Ui>
